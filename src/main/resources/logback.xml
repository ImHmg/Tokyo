<configuration>
    <!-- Disable all logs by setting the root level to OFF -->
    <root level="OFF">
        <appender-ref ref="CONSOLE" />
    </root>

    <!-- Optional: Define your appenders if needed -->
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
</configuration>
